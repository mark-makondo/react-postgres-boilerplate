{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "quotes": [
            "error",
            "double"
        ],
        "indent": [
            "error",
            4
        ],
        "comma-dangle": [
            "error",
            "never"
        ],
        "consistent-return": "off",
        "import/extensions": [
            "error",
            "ignorePackages"
        ]
    }
}